        <activity android:name=".activities.FeedbackActivity" />
        <activity android:name=".activities.MessageActivity" />
        <activity android:name=".activities.InviteFriendsActivity" />
        <activity android:name=".activities.NotificationActivity" />
        <activity android:name=".activities.PromoCodeListActivity" />
        <activity android:name=".activities.LookingDriverActivity" />
        <activity android:name=".activities.DeliveryDetailActivity" />
        <activity android:name=".activities.PickUpDetailActivity" />

        <activity android:name=".activities.AddCardScreen" />
        <activity android:name=".activities.BalanceActivity" />
        <activity android:name=".activities.MyWallet" />
        <activity android:name=".MainActivity" />
        <activity android:name=".activities.OtpActivity" />
        <activity android:name=".activities.VerificationActivity" />
        <activity android:name=".activities.SignupActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.OptionActivity" />
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/googleApiKey" />
    </application>

</manifest>